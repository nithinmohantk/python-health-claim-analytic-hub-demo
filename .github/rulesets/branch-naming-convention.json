{
  "name": "Branch Naming Convention",
  "target": "branch",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "include": [
        "refs/heads/feature/*",
        "refs/heads/bugfix/*",
        "refs/heads/hotfix/*",
        "refs/heads/release/*",
        "refs/heads/docs/*",
        "refs/heads/refactor/*",
        "refs/heads/test/*"
      ],
      "exclude": [
        "refs/heads/main",
        "refs/heads/develop"
      ]
    }
  },
  "rules": [
    {
      "type": "creation",
      "parameters": {
        "allowed_patterns": [
          "^feature/[a-z0-9-]+$",
          "^bugfix/[a-z0-9-]+$",
          "^hotfix/[a-z0-9-]+$",
          "^release/[a-z0-9.-]+$",
          "^docs/[a-z0-9-]+$",
          "^refactor/[a-z0-9-]+$",
          "^test/[a-z0-9-]+$"
        ],
        "blocked_patterns": []
      }
    }
  ],
  "bypass_actors": []
}
